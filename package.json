{
  "name": "@zowieteam/zowie-agent-sdk",
  "version": "0.1.0",
  "description": "Node.js/TypeScript SDK for building external agents that integrate with Zowie Decision Engine",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx watch example.ts",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "test:real": "NODE_OPTIONS='--experimental-vm-modules' jest tests/e2e.real-apis.test.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "typecheck": "tsc --project tsconfig.check.json",
    "check": "pnpm run lint && pnpm run typecheck && pnpm run test",
    "prepublishOnly": "pnpm run build",
    "clean": "rm -rf dist",
    "install:clean": "rm -rf node_modules pnpm-lock.yaml && pnpm install"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "zowie",
    "agent",
    "sdk",
    "typescript",
    "ai",
    "llm",
    "openai",
    "google",
    "gemini"
  ],
  "author": {
    "name": "zowie",
    "email": "support@zowie.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zowie-ai/zowie-agent-sdk-node.git"
  },
  "bugs": {
    "url": "https://github.com/zowie-ai/zowie-agent-sdk-node/issues"
  },
  "homepage": "https://github.com/zowie-ai/zowie-agent-sdk-node#readme",
  "dependencies": {
    "@google/genai": "^1.20.0",
    "express": "^5.1.0",
    "openai": "^5.20.3",
    "winston": "^3.17.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@jest/globals": "^29.7.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.16",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
